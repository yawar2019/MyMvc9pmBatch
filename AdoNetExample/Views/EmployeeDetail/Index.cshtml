@model IEnumerable<AdoNetExample.Models.EmployeeModel>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
          EmpId
        </th>
        <th>
           EmpName
        </th>
        <th>
           EmpSalary
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
         @item.EmpId
        </td>
        <td>
         @item.EmpName
        </td>
        <td>
         @item.EmpSalary
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new {  id=item.EmpId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.EmpId })
        </td>
    </tr>
}

</table>


